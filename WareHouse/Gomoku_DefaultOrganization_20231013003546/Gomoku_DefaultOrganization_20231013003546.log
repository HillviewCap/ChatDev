        urls = re.findall(r"http[s]?://(?:[a-zA-Z]|[0-9]|[$-_@.&+]|[!*\\(\\),]|(?:%[0-9a-fA-F][0-9a-fA-F]))+", email)
        return urls
    def generate_report(self):
        with open("report.txt", "w") as f:
            f.write(self.report)
    def get_report(self):
        return self.report
    def get_whitelist(self):
        '''
